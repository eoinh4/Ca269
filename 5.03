public class Date
{
    private String day;
    private String month;
    private String year;
    
    public String toString()
    {
        return day + "/" + month + "/" + year;
    }

    public Date(String date1)
    {
        String[] parts = date1.split(" ");
        this.day = parts[0];
        this.month = parts[1];
        this.year = parts[2];
    }
}
