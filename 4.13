public class Test
{
    public static void printForties(Student [] student)
    {
        for(int i = 0; i < student.length;i++)
        {
        int mark = student[i].getMark();
            if( mark >= 40 && mark < 50)
            {
                System.out.println(student[i].getMark() + " (" + student[i].getName() + ")");
            }
        }
    }
}
