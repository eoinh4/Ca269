public class Test
{
    public static int numberPasses(Student [] student)
    {
        int numPassed = 0;
        for(int i = 0; i < student.length;i++)
        {
        int mark = student[i].getMark();
            if( mark >= 40)
            {
                numPassed++;
            }
        }
        return numPassed;
    }
}
