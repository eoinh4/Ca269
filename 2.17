import java.util.Scanner;

public class NoMinusOne
{
    public static void main(String [] args)
    {
        Scanner in = new Scanner(System.in);
        int prev = 0;
        System.out.println("Enter numbers: ");
        int current = 0;
        while(current != -1)
        {
            prev = current;
            current = in.nextInt();
        }
        System.out.println("The penultimate number was: " + prev);
    }
}
