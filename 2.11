import java.util.Scanner;
public class NumToTwenty
{
    public static void main(String [] args)
    {
        Scanner in = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int input = in.nextInt();
        while (input <= 20)
        {
            System.out.print(input + " ");
            input++;
        }
        System.out.print("\n");
    }
}
